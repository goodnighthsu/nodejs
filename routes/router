var express = require('express');
var router = express.Router();
var routeUser = require('./routeUser');


var routes = function(app)
{
    /* GET home page. */
    router.get('/', function(req, res, next) {
        res.render('index', { title: 'Express' });
    });
    app.use(router);

    // api/user
    app.use(routeUser);
}

module.exports = routes;